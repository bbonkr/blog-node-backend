{
    "name": "blog-node-backend",
    "version": "1.0.9",
    "description": "blog service backend",
    "main": "app.js",
    "keywords": [],
    "author": "Pon Cheol Ku <bbon@bbon.kr>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/bbonkr/blog-node-backend.git"
    },
    "scripts": {
        "sequelize": "node_modules/.bin/sequelize",
        "test": "cross-env NODE_ENV=test JWT_SECRET=TEST JWT_ISSUER=TEST JWT_AUDIENCE=TEST COOKIE_SECRET=TEST jest --forceExit --detectOpenHandles",
        "coverage": "cross-env NODE_ENV=test jest --coverage",
        "cover": "cross-env NODE_ENV=test JWT_SECRET=TEST JWT_ISSUER=TEST JWT_AUDIENCE=TEST COOKIE_SECRET=TEST jest --forceExit --detectOpenHandles --coverage",
        "lint": "tslint src/**/*.ts{,x} --config ./tslint.json --fix",
        "lint:dev": "cross-env NODE_ENV=development npm run lint",
        "lint:prod": "cross-env NODE_ENV=production npm run lint",
        "bundle": "webpack",
        "bundle:prod": "cross-env NODE_ENV=production npm run bundle",
        "build": "tsc",
        "build:dev": "cross-env NODE_ENV=development npm run build",
        "build:prod": "cross-env NODE_ENV=production npm run build",
        "dev": "npm run lint:dev && npm run build:dev && cross-env PORT=5000 NODE_ENV=development node ./dist/index.js",
        "prestart": "npm run lint",
        "start": "npm run lint:prod && npm run bundle:prod && cross-env PORT=5000 NODE_ENV=production node ./bundle/index.js"
    },
